{
  "name": "@pascalhonegger/ng-datatable",
  "version": "8.0.0",
  "description": "DataTable component for Angular framework",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "ngc -p src",
    "start": "http-server -c-1 -o -p 8875 .",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage-remap": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-remapped.lcov -t lcovonly",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PascalHonegger/ng-datatable.git"
  },
  "keywords": [
    "angular",
    "angular8",
    "ng",
    "ng2",
    "table",
    "pagination",
    "sort",
    "sorting"
  ],
  "author": "pascal.honegger@bluewin.ch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PascalHonegger/ng-datatable/issues"
  },
  "homepage": "https://github.com/cmglez10/ng-datatable#readme",
  "devDependencies": {
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@types/jasmine": "3.3.7",
    "@types/lodash": "4.14.144",
    "core-js": "^2.6.10",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.16",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "systemjs": "^0.21.4",
    "typescript": "3.5.x",
    "zone.js": "~0.9.1"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "@angular/core": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "rxjs": "^6.0.0"
  }
}
